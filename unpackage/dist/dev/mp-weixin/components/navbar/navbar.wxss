
.navbar {
	position: fixed;
	left: 0;
	top: 0;
	display: flex;
	align-items: center;
	justify-content: space-between;
	background-color: #fff;
	z-index: 9999;
	box-shadow: 0 1px 2px rgba(0, 0, 0, 0.05);
	padding-left: 48rpx;
	padding-right: 100px;
}
.need-back {
	justify-content: flex-start;
}
.back {
	width: 44px;
	height: 44px;
	display: flex;
	justify-content: center;
	align-items: center;
}
.title {
	font-size: 36rpx;
	color: #262626;
	font-weight: 400;
}
.right {
	width: 44px;
	height: 44px;
	display: flex;
	justify-content: center;
	align-items: center;
}

