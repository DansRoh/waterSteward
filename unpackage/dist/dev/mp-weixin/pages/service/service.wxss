.page-service {
  height: 100vh;
  box-sizing: border-box;
  background-color: #F2F4F7;
}
.page-service .custom-grid-item {
  border-radius: 16rpx;
}
.page-service .voice-msg {
  width: 658rpx;
  margin: 0 auto;
}
.page-service .voice-msg .voice-msg-textarea {
  width: 658rpx;
  box-sizing: border-box;
  height: 480rpx;
  padding: 38rpx 42rpx;
  background-color: #fff;
  border: 2rpx solid #BDBDBD;
  border-radius: 8rpx;
}

