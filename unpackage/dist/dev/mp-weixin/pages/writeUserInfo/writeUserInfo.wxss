
.navbar {
	position: fixed;
	left: 0;
	top: 0;
	display: flex;
	align-items: center;
	justify-content: space-between;
	background-color: #fff;
	z-index: 9999;
	box-shadow: 0 1px 2px rgba(0, 0, 0, 0.05);
	padding-left: 48rpx;
	padding-right: 100px;
}
.need-back {
	justify-content: flex-start;
}
.back {
	width: 44px;
	height: 44px;
	display: flex;
	justify-content: center;
	align-items: center;
}
.title {
	font-size: 36rpx;
	color: #262626;
	font-weight: 400;
}
.right {
	width: 44px;
	height: 44px;
	display: flex;
	justify-content: center;
	align-items: center;
}

.page-writeUserInfo {
  box-sizing: border-box;
  padding: 0 48rpx;
}
.page-writeUserInfo .title {
  padding: 70rpx 0;
}
.page-writeUserInfo .title .title-content {
  font-size: 48rpx;
  font-weight: 600;
  color: #262626;
}
.page-writeUserInfo .title .title-tip {
  font-size: 24rpx;
  font-weight: 400;
  color: #17DA9C;
}
.page-writeUserInfo .form-user-info .form-item {
  display: flex;
}
.page-writeUserInfo .form-user-info .form-item .item-label {
  min-width: 104rpx;
  line-height: 88rpx;
  color: #828698;
  font-size: 28rpx;
}
.page-writeUserInfo .form-user-info .form-item .label-name {
  text-align: justify;
  text-align-last: justify;
}
.page-writeUserInfo .form-user-info .form-item .custom-field {
  width: 462rpx;
  font-size: 36rpx;
  color: #262626;
}
.page-writeUserInfo .form-user-info .form-item .custom-picker-class {
  padding: 10px 16px;
}
.page-writeUserInfo .form-user-info .form-item .custom-phone {
  padding-left: 80rpx;
  position: relative;
}
.page-writeUserInfo .form-user-info .form-item .custom-phone::after {
  content: "+86";
  display: block;
  position: absolute;
  left: 0;
  top: 0;
  color: #828698;
  font-size: 32rpx;
}
.page-writeUserInfo .form-user-info .transaction-btn {
  flex: 1;
  margin-top: 56rpx;
}

