.page-devManage {
  min-height: 100vh;
  background-color: #F2F4F7;
  padding: 50rpx 40rpx;
}
.page-devManage .dev-card {
  width: 666rpx;
  background-color: #fff;
  box-shadow: 4rpx 8rpx 48rpx 0rpx rgba(0, 0, 0, 0.17);
  border-radius: 28rpx;
  box-sizing: border-box;
  padding: 46rpx 26rpx;
}
.page-devManage .dev-card .dev-name {
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.page-devManage .dev-card .dev-handle {
  display: flex;
  justify-content: flex-end;
}

