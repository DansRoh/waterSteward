.page-accountSafe {
  min-height: 100vh;
  background-color: #F2F4F7;
  display: flex;
  flex-direction: column;
  align-items: center;
  padding-bottom: 80rpx;
  box-sizing: border-box;
}
.page-accountSafe .info-list {
  width: 100vw;
  padding-top: 10rpx;
}
.page-accountSafe .info-list .info-item {
  margin-top: 16rpx;
}
.page-accountSafe .info-list .cus-value {
  font-size: 36rpx;
  color: #828698;
}
.page-accountSafe .info-list .right-icon-style {
  margin-left: 40rpx;
}
.page-accountSafe .log-out {
  position: fixed;
  bottom: 100rpx;
  left: 50%;
  -webkit-transform: translateX(-50%);
          transform: translateX(-50%);
  font-size: 28rpx;
  color: #FFA212;
}

